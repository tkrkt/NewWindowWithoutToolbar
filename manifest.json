{
  "manifest_version": 2,
  "name": "New window without toolbar",
  "version": "0.1.1",
  "description": "open current page in new window without toolbar",
  "author": "tkrkt",
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}